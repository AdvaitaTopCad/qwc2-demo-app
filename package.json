{
    "name": "QWC2App",
    "version": "1.1.0",
    "description": "QGIS Web Client 2 application",
    "author": "Sourcepole",
    "license": "BSD-2-Clause",
    "repository": "git@github.com:sourcepole/qwc2-demo-app.git",
    "private": true,
    "devDependencies": {
        "@babel/core": "7.9.0",
        "@babel/plugin-proposal-class-properties": "7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "7.9.5",
        "@babel/preset-env": "7.9.5",
        "@babel/preset-react": "7.9.4",
        "babel-eslint": "10.1.0",
        "babel-loader": "8.1.0",
        "css-loader": "3.5.2",
        "eslint-plugin-react": "7.19.0",
        "eslint": "6.8.0",
        "file-loader": "6.0.0",
        "mkdirp": "1.0.4",
        "object-path": "^0.11.4",
        "redux-devtools-dock-monitor": "1.1.3",
        "redux-devtools-log-monitor": "1.4.0",
        "redux-devtools": "3.5.0",
        "redux-immutable-state-invariant": "2.1.0",
        "rimraf": "3.0.2",
        "string-replace-loader": "2.3.0",
        "style-loader": "1.1.4",
        "url-loader": "4.1.0",
        "webfonts-generator": "0.4.0",
        "webpack-bundle-size-analyzer": "3.1.0",
        "webpack-cli": "3.3.11",
        "webpack-dev-server": "3.10.3",
        "webpack": "4.43.0",
        "xml2js": "0.4.19"
    },
    "scripts": {
        "clean": "rimraf dist",
        "build": "node -e \"process.exit(require('os').platform() === 'win32' ? 0 : 1)\" && node_modules\\qwc2\\scripts\\package-commands.bat build || ./node_modules/qwc2/scripts/package-commands.sh build",
        "start": "npm run tsupdate && npm run themesconfig && npm run iconfont && webpack-dev-server --progress --colors --host 0.0.0.0 --port 8081 --config ./webpack.config.js",
        "iconfont": "node node_modules/qwc2/scripts/makeIconkit.js",
        "themesconfig": "node node_modules/qwc2/scripts/themesConfig.js",
        "prod": "node -e \"process.exit(require('os').platform() === 'win32' ? 0 : 1)\" && node_modules\\qwc2\\scripts\\package-commands.bat prod || ./node_modules/qwc2/scripts/package-commands.sh prod",
        "tsupdate": "node node_modules/qwc2/scripts/updateTranslations.js",
        "analyze": "node -e \"process.exit(require('os').platform() === 'win32' ? 0 : 1)\" && node_modules\\qwc2\\scripts\\package-commands.bat analyze || ./node_modules/qwc2/scripts/package-commands.sh analyze",
        "release": "node -e \"process.exit(require('os').platform() === 'win32' ? 0 : 1)\" && node_modules\\qwc2\\scripts\\package-commands.bat release || ./node_modules/qwc2/scripts/package-commands.sh release"
    },
    "dependencies": {
        "qwc2": "^1.1.0"
    }
}

{
  "proxyServiceUrl": "http://localhost:5000/",
  "permalinkServiceUrl": "http://localhost:5001/",
  "elevationServiceUrl": "http://localhost:5002/",
  "mapInfoService":  "http://localhost:8093/",
  "featureReportService": "http://localhost:5020/",
  "translationsPath": "/translations",
  "assetsPath": "/assets",
  "urlPositionFormat": "extent",
  "urlPositionCrs": "",
  "preserveExtentOnThemeSwitch": true,
  "preserveBackgroundOnThemeSwitch": true,
  "preserveNonThemeLayersOnThemeSwitch": true,
  "allowReorderingLayers": true,
  "allowRemovingThemeLayers": true,
  "wmsDpi": 96,
  "defaultFeatureStyle": {
    "strokeColor": [0, 0, 255, 1],
    "strokeWidth": 1,
    "strokeDash": [4],
    "fillColor": [255, 0, 255, 0.33],
    "circleRadius": 10,
    "textFill": "black",
    "textStroke": "white"
  },
  "plugins": {
      "mobile": [
        {
          "name": "Map",
          "toolsOptions": {
            "OverviewSupport": {
              "tipLabel": "Overview"
            },
            "LocateSupport": {
              "keepCurrentZoomLevel": true
            },
            "ScaleBarSupport": {
              "units": "metric"
            }
          }
        },
        {
          "name": "HomeButton"
          ,
          "cfg": {
            "position": 2
          }
        },
        {
          "name": "LocateButton",
          "cfg": {
            "position": 1
          }
        },
        {
          "name": "BackgroundSwitcher",
          "cfg": {
            "position": 0
          }
        },
        {
            "name": "TopBar",
            "cfg": {
              "menuItems": [
                {"key": "ThemeSwitcher", "icon": "themes"},
                {"key": "LayerTree", "icon": "layers"},
                {"key": "Share", "icon": "share"},
                {"key": "Tools", "icon": "tools", "subitems": [
                  {"key": "IdentifyRegion", "icon": "identify_region"},
                  {"key": "Measure", "icon": "measure"},
                  {"key": "Redlining", "icon": "draw"},
                  {"key": "Editing", "icon": "editing"},
                  {"key": "DxfExport", "icon": "dxfexport"},
                  {"key": "RasterExport", "icon": "rasterexport"}
                ]},
                {"key": "Print", "icon": "print"},
                {"key": "Help", "icon": "info"}
              ],
              "searchOptions": {
                "minScale": 1000,
                "showProviderSelection": true,
                "providerSelectionAllowAll": true,
                "zoomToLayers": false,
                "showProvidersInPlaceholder": false
              },
              "appMenuClearsTask": true,
              "logoClickResetsTheme": true
            }
        },
        {
          "name": "ThemeSwitcher"
        },
        {
          "name": "Measure",
          "cfg": {
            "showMeasureModeSwitcher": true
          }
        },
        {
          "name": "Identify",
          "cfg": {
            "params": {
              "FI_POINT_TOLERANCE": 32,
              "FI_LINE_TOLERANCE": 16,
              "FI_POLYGON_TOLERANCE": 8
            },
            "enableExport": true,
            "longAttributesDisplay": "ellipsis",
            "displayResultTree": true
          }
        },
        {
          "name": "Share",
          "cfg": {
            "showSocials": true,
            "showLink": true,
            "showQRCode": true
          }
        },
        {
          "name": "Print"
        },
        {
          "name": "Help"
        },
        {
          "name": "MapCopyright"
        },
        {
          "name": "LayerTree",
          "cfg": {
            "showLegendIcons": true,
            "showRootEntry": true,
            "showQueryableIcon": true,
            "groupTogglesSublayers": false,
            "grayUnchecked": false,
            "flattenGroups": false,
            "layerInfoWindowSize": {"width": 480, "height": 400}
          }
        },
        {
          "name": "DxfExport"
        },
        {
          "name": "RasterExport",
          "cfg": {
            "dpis": [96, 300]
          }
        },
        {
          "name": "Redlining"
        },
        {
          "name": "Editing"
        },
        {
          "name": "MapCompare"
        },
        {
          "name": "HeightProfile",
          "cfg": {
            "heighProfilePrecision": 0
          }
        },
        {
          "name": "MapInfoTooltip",
          "cfg": {
            "elevationPrecision": 0
          }
        },
        {
          "name": "IdentifyRegion",
          "cfg": {
            "useWfs": false
          }
        }
      ],
      "desktop": [
        {
          "name": "Map",
          "cfg": {
            "toolsOptions": {
              "OverviewSupport": {
                "tipLabel": "Overview"
              },
              "LocateSupport": {
                "keepCurrentZoomLevel": true
              },
              "ScaleBarSupport": {
                "units": "metric"
              }
            }
          }
        },
        {
          "name": "HomeButton"
          ,
          "cfg": {
            "position": 4
